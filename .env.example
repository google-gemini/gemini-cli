# ==============================================
# GEMINI CLI - ENVIRONMENT TEMPLATE
# ==============================================
# Copy to .env and fill with real values
# NEVER commit real credentials to version control!
# ==============================================

# ==============================================
# GOOGLE AI / VERTEX AI
# ==============================================
GOOGLE_API_KEY={{GOOGLE_API_KEY}}
GOOGLE_APPLICATION_CREDENTIALS={{GOOGLE_APPLICATION_CREDENTIALS}}
GOOGLE_PROJECT_ID={{GOOGLE_PROJECT_ID}}

# Vertex AI specific
VERTEX_AI_LOCATION={{VERTEX_AI_LOCATION}}
VERTEX_AI_PROJECT={{VERTEX_AI_PROJECT}}

# ==============================================
# CLI CONFIGURATION
# ==============================================
CLI_LOG_LEVEL=info
CLI_OUTPUT_FORMAT=json

# ==============================================
# MODEL SETTINGS
# ==============================================
GEMINI_MODEL=gemini-1.5-flash
TEMPERATURE=0.7
MAX_OUTPUT_TOKENS=1024
TOP_K=40
TOP_P=0.95

# ==============================================
# SAFETY SETTINGS
# ==============================================
SAFETY_HARASSMENT=BLOCK_MEDIUM_AND_ABOVE
SAFETY_HATE=BLOCK_MEDIUM_AND_ABOVE
SAFETY_SEXUALLY_EXPLICIT=BLOCK_MEDIUM_AND_ABOVE
SAFETY_DANGEROUS=BLOCK_MEDIUM_AND_ABOVE
