# Accelos AI Agent Environment Variables

# LLM API Keys
GOOGLE_API_KEY=your_google_api_key_here
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Guardrails and RCA Configuration
ACCELOS_GUARDRAIL_FILE_PATH=./src/prompts/guardrails.json
ACCELOS_DATA_DIRECTORY_PATH=./src/rcas
RCA_DIRECTORY_PATH=./src/rcas

# GitHub MCP Configuration
GITHUB_TOKEN=your_github_personal_access_token_here
GITHUB_MCP_SERVER_URL=http://localhost:3001/mcp
# Alternative: For subprocess mode
# GITHUB_MCP_COMMAND=npx github-mcp-server

# Mastra Server Configuration
MASTRA_PORT=4111
MASTRA_HOST=0.0.0.0

# Neo4j Database Configuration
NEO4J_URI=bolt://localhost:7689
NEO4J_USERNAME=neo4j
NEO4J_PASSWORD=pass-here
NEO4J_DATABASE=neo4j