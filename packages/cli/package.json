{
  "name": "@gemini-code/cli",
  "version": "0.1.0",
  "description": "Gemini Code CLI",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "gemini-code": "dist/index.js"
  },
  "scripts": {
    "build": "tsc --build && touch dist/.last_build",
    "clean": "rm -rf dist",
    "start": "node dist/gemini.js",
    "debug": "node --inspect-brk dist/gemini.js",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "test": "vitest run",
    "stage:version": "node ../../scripts/bind_package_version.js",
    "stage:deps": "node ../../scripts/bind_package_dependencies.js"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@gemini-code/server": "*",
    "@google/genai": "^0.8.0",
    "diff": "^7.0.0",
    "dotenv": "^16.4.7",
    "fast-glob": "^3.3.3",
    "highlight.js": "^11.11.1",
    "ink": "^5.2.0",
    "ink-gradient": "^3.0.0",
    "ink-select-input": "^6.0.0",
    "ink-spinner": "^5.0.0",
    "ink-text-input": "^6.0.0",
    "lowlight": "^3.3.0",
    "react": "^18.3.1",
    "read-package-up": "^11.0.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/diff": "^7.0.2",
    "@types/dotenv": "^6.1.1",
    "@types/node": "^20.11.24",
    "@types/react": "^19.1.0",
    "@types/yargs": "^17.0.32",
    "typescript": "^5.3.3",
    "vitest": "^3.1.1"
  },
  "engines": {
    "node": ">=18"
  }
}
