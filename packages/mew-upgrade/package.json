{
  "name": "@google/gemini-cli-mew-upgrade",
  "version": "0.1.21",
  "description": "Mew Agent and TUI",
  "type": "module",
  "main": "dist/agent/gemini-agent.js",
  "exports": {
    "./agent/gemini-agent.js": "./dist/agent/gemini-agent.js",
    "./agent/gemini-agent.d.ts": "./dist/agent/gemini-agent.d.ts",
    "./mind/mental-laby.js": "./dist/mind/mental-laby.js",
    "./mind/mental-laby.d.ts": "./dist/mind/mental-laby.d.ts",
    "./mind/*": "./dist/mind/*",
    "./agent/*": "./dist/agent/*"
  },
  "scripts": {
    "start:app": "tsx src/app/index.ts",
    "start:web": "tsx src/server/webServer.ts",
    "build:web": "esbuild src/app/web.tsx --bundle --outfile=public/web.js --loader:.js=jsx --target=es2020",
    "build": "tsc && npm run build:web",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@google/gemini-cli-core": "file:../core",
    "ink": "^6.1.1",
    "react": "^19.1.0",
    "express": "^4.19.2"
  },
  "devDependencies": {
    "@types/react": "^19.1.8",
    "tsx": "^4.20.3",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20"
  }
}
